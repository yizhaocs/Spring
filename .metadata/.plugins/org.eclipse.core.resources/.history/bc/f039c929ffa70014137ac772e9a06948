	<bean id="triangle" class="com.yizhao.model.Triangle" name="triangle-name">
		<property name="points">
			<list>
				<ref bean="pointA" />
				<ref bean="pointB" />
				<ref bean="pointC" />
			</list>
		</property>


		<property name="pointA">
			<bean class="com.yizhao.model.Point">
				<property name="x" value="0" />
				<property name="y" value="0" />
			</bean>
		</property>

		<property name="pointB">
			<bean class="com.yizhao.model.Point">
				<property name="x" value="-20" />
				<property name="y" value="0" />
			</bean>
		</property>

		<property name="pointC">
			<bean class="com.yizhao.model.Point">
				<property name="x" value="20" />
				<property name="y" value="0" />
			</bean>
		</property>
	</bean>
</beans>
